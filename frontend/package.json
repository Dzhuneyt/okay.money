{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "npx concurrently 'npm run build:watch' 'npm run util:lite-server' ",
    "start": "npx ng build",
    "serve": "npx ng serve",
    "build": "npx ng build",
    "util:lite-server": "npx lite-server --baseDir='dist/frontend'",
    "build:watch": "npx ng build --watch=true --progress=true --poll 1200",
    "build:prod": "npx ng build --prod",
    "test": "npx ng test --sourceMap=false",
    "test:cli": "npx ng test --sourceMap=false --browsers=ChromeHeadless --watch=false",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngx-pwa/local-storage": "^7.4.0",
    "chart.js": "^2.8.0",
    "concurrently": "^5.2.0",
    "core-js": "^2.6.9",
    "lite-server": "^2.5.4",
    "ng2-charts": "2.2.3",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.5",
    "time-ago-pipe": "^1.3.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.26",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
