{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "docker run -it --rm -w /app -v $(pwd):/app:delegated -p 80:4200 alexsuch/angular-cli:10.2.3 ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start": "npx ng build",
    "serve": "npx ng serve --proxy-config proxy.conf.json",
    "build": "npx ng build",
    "util:lite-server": "npx lite-server",
    "build:watch": "npx ng build --watch=true --progress=true",
    "build:prod": "npx ng build --configuration production",
    "build:prod:watch": "npx ng build --configuration production --watch",
    "test": "npx ng test --sourceMap=false",
    "test:cli": "npx ng test --sourceMap=false --browsers=ChromeHeadless --watch=false",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.6",
    "@angular/cdk": "^13.3.6",
    "@angular/common": "^13.3.6",
    "@angular/compiler": "^13.3.6",
    "@angular/core": "^13.3.6",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.3.6",
    "@angular/material": "^13.3.6",
    "@angular/platform-browser": "^13.3.6",
    "@angular/platform-browser-dynamic": "^13.3.6",
    "@angular/router": "^13.3.6",
    "@ngx-pwa/local-storage": "^13.0.3",
    "chart.js": "^3.7.1",
    "concurrently": "^7.1.0",
    "core-js": "^3.22.4",
    "http-proxy-middleware": "^2.0.6",
    "lite-server": "^2.6.1",
    "luxon": "^2.3.2",
    "ng2-charts": "^3.0.10",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.5.5",
    "time-ago-pipe": "^1.3.2",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.6",
    "@angular/language-service": "^13.3.6",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.31",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
