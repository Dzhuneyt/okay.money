{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "npx concurrently 'npm run build:watch' 'npm run util:lite-server' ",
    "start": "npx ng build",
    "serve": "npx ng serve --proxy-config proxy.conf.json",
    "build": "npx ng build",
    "util:lite-server": "npx lite-server",
    "build:watch": "npx ng build --watch=true --progress=true",
    "build:prod": "npx ng build --configuration production",
    "build:prod:watch": "npx ng build --configuration production --watch",
    "test": "npx ng test --sourceMap=false",
    "test:cli": "npx ng test --sourceMap=false --browsers=ChromeHeadless --watch=false",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.3",
    "@angular/cdk": "^13.2.3",
    "@angular/common": "^13.2.3",
    "@angular/compiler": "^13.2.3",
    "@angular/core": "^13.2.3",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^13.2.3",
    "@angular/material": "^13.2.3",
    "@angular/platform-browser": "^13.2.3",
    "@angular/platform-browser-dynamic": "^13.2.3",
    "@angular/router": "^13.2.3",
    "@ngx-pwa/local-storage": "^11.0.1",
    "chart.js": "^2.8.0",
    "concurrently": "^5.2.0",
    "core-js": "^2.6.9",
    "http-proxy-middleware": "^1.0.6",
    "lite-server": "^2.6.1",
    "ng2-charts": "^2.4.2",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.3",
    "time-ago-pipe": "^1.3.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.4",
    "@angular/cli": "^13.2.4",
    "@angular/compiler-cli": "^13.2.3",
    "@angular/language-service": "^13.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
