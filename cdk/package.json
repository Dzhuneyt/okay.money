{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "deploy": "npm run deploy:backend",
    "deploy:backend": "cdk deploy '*-backend' --require-approval=never --outputs-file ./outputs.json",
    "deploy:frontend": "cdk deploy '*-frontend' --require-approval=never --outputs-file ./outputs.json",
    "deploy:ci": "cdk deploy '*' --app 'npx ts-node --prefer-ts-exts bin/ci.ts' --require-approval=never --outputs-file ./ci-outputs.json",
    "test": "jest",
    "cdk": "cdk",
    "build": "cdk synth"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.83.0",
    "@types/aws-lambda": "^8.10.70",
    "@types/jest": "^25.2.3",
    "@types/node": "10.17.5",
    "aws-cdk": "^1.83.0",
    "esbuild": "^0.8.30",
    "jest": "^25.5.4",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.83.0",
    "@aws-cdk/aws-cloudfront": "^1.83.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.83.0",
    "@aws-cdk/aws-cloudwatch": "^1.83.0",
    "@aws-cdk/aws-codebuild": "^1.83.0",
    "@aws-cdk/aws-cognito": "^1.83.0",
    "@aws-cdk/aws-dynamodb": "^1.83.0",
    "@aws-cdk/aws-iam": "^1.83.0",
    "@aws-cdk/aws-lambda": "^1.83.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.83.0",
    "@aws-cdk/aws-logs": "^1.83.0",
    "@aws-cdk/aws-s3": "^1.83.0",
    "@aws-cdk/aws-s3-deployment": "^1.83.0",
    "@aws-cdk/aws-ssm": "^1.83.0",
    "@aws-cdk/core": "^1.83.0",
    "@aws-cdk/pipelines": "^1.83.0",
    "@sendgrid/mail": "^7.4.0",
    "@types/uuid": "^7.0.4",
    "aws-sdk": "^2.823.0",
    "class-validator": "^0.12.2",
    "source-map-support": "^0.5.16",
    "ts-loader": "^7.0.5",
    "uuid": "^8.3.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "cdk-lambda": "/asset-output/index.js",
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}
