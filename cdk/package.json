{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "deploy": "npm run deploy:backend",
    "deploy:backend": "cdk deploy '*-backend' --require-approval=never --outputs-file ./outputs.json",
    "deploy:frontend": "cdk deploy '*-frontend' --require-approval=never --outputs-file ./outputs.json",
    "deploy:ci": "cdk deploy '*' --app 'npx ts-node --prefer-ts-exts bin/ci.ts' --require-approval=never --outputs-file ./ci-outputs.json",
    "test": "jest",
    "cdk": "cdk",
    "build": "cdk synth"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.133.0",
    "@types/aws-lambda": "^8.10.85",
    "@types/jest": "^27.0.3",
    "@types/node": "16.11.9",
    "aws-cdk": "^1.133.0",
    "esbuild": "^0.13.15",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.133.0",
    "@aws-cdk/aws-certificatemanager": "^1.133.0",
    "@aws-cdk/aws-cloudfront": "^1.133.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.133.0",
    "@aws-cdk/aws-cloudwatch": "^1.133.0",
    "@aws-cdk/aws-codebuild": "^1.133.0",
    "@aws-cdk/aws-cognito": "^1.133.0",
    "@aws-cdk/aws-dynamodb": "^1.133.0",
    "@aws-cdk/aws-iam": "^1.133.0",
    "@aws-cdk/aws-lambda": "^1.133.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.133.0",
    "@aws-cdk/aws-logs": "^1.133.0",
    "@aws-cdk/aws-route53": "^1.133.0",
    "@aws-cdk/aws-route53-targets": "^1.133.0",
    "@aws-cdk/aws-s3": "^1.133.0",
    "@aws-cdk/aws-s3-deployment": "^1.133.0",
    "@aws-cdk/aws-ssm": "^1.133.0",
    "@aws-cdk/core": "^1.133.0",
    "@aws-cdk/pipelines": "^1.133.0",
    "@sendgrid/mail": "^7.6.0",
    "@types/uuid": "^8.3.3",
    "aws-sdk": "^2.1034.0",
    "class-validator": "^0.13.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.2.6",
    "uuid": "^8.3.2",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  },
  "cdk-lambda": "/asset-output/index.js",
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}
