{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
  ],
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "216987438199.dkr.ecr.us-east-1.amazonaws.com/finance/nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "backend",
        "frontend"
      ],
      "mountPoints": [
      ]
    },
    {
      "name": "backend",
      "image": "216987438199.dkr.ecr.us-east-1.amazonaws.com/finance/backend",
      "essential": true,
      "memory": 128,
      "Volumes": [
        {
          "HostDirectory": "/tmp/",
          "ContainerDirectory": "/host_tmp/"
        }
      ]
    },
    {
      "name": "migrations",
      "image": "216987438199.dkr.ecr.us-east-1.amazonaws.com/finance/backend",
      "essential": true,
      "memory": 128,
      "command": [
        "sleep 60"
      ],
      "Volumes": [
        {
          "HostDirectory": "/tmp/",
          "ContainerDirectory": "/host_tmp/"
        }
      ]
    },
    {
      "name": "frontend",
      "image": "216987438199.dkr.ecr.us-east-1.amazonaws.com/finance/frontend",
      "essential": true,
      "memory": 128
    }
  ]
}
